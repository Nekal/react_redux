{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
    },
  "extends": ["react", "airbnb-base"],
  "rules" : {
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "no-console":0,
    "comma-dangle": ["error", "never"],
    "no-unused-expressions": 0,
    "no-use-before-define": ["error", { "functions": false }]
  },
  "env": {
    "node": true,
    "mocha": true,
    "browser": true
  }
}
